{
    "name": "oro/segmentation-tree-bundle",
    "type": "symfony-bundle",
    "description": "BAP Segmentation Tree Bundle",
    "keywords": ["BAP", "Tree", "Segmentation"],
    "homepage": "https://github.com/laboro/SegmentationTreeBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "stof/doctrine-extensions-bundle": "dev-master",
        "oro/requirejs-bundle": "dev-master"
    },
    "require-dev": {
        "doctrine/dbal": ">=2.2,<2.4-dev",
        "doctrine/orm": ">=2.2.3,<2.4-dev"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\SegmentationTreeBundle": "" }
    },
    "target-dir": "Oro/Bundle/SegmentationTreeBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
